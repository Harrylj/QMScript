// 1随机 2加血 5地牢 F4隐身 F7治疗术 F8召唤

Delay 10
BeginThread A
Delay 10
BeginThread B
Delay 10
BeginThread C
Delay 10
BeginThread D

// 子程序不支持调用外部参数

// 有蓝有血 移动再飞随机 不移动
Sub A()
    While true
        x = 400
        y = 400
        bgColor = "ffffff"
        // 颜色 = GetPixelColor(x,y)
        // 秒 = Second(now)
        // SayString 颜色
        // SayString 秒
        IfColor x, y, bgColor, 0 Then
        	MessageBox 1
            Delay 2000
            MessageBox 11
        Else
        End If
        Delay 500
    Wend
End Sub

// 减少血，加血
Sub B()
    While true
        x = 400
        y = 400
        bgColor = "ffffff"
        IfColor x - 3, y, bgColor, 1 Then
        	MessageBox 2
            Delay 2000
            MessageBox 22
        Else
        End If
        Delay 500
    Wend
End Sub

